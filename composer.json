{
    "name": "itomic/teamwork",
    "description": "PHP wrapper for the Teamwork project management API",
    "keywords": ["teamwork", "api", "laravel", "project management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Edman",
            "email": "ross.edman@gmail.com"
        },
        {
            "name": "Nigel Heap",
            "email": "nigel@nigelheap.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-0": {
            "NigelHeap\\Teamwork\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NigelHeap\\Teamwork\\TeamworkServiceProvider"
            ],
            "aliases": {
                "Teamwork": "NigelHeap\\Teamwork\\Facades\\Teamwork"
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for Laravel support"
    },
    "minimum-stability": "stable"
}
